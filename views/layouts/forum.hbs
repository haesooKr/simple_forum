<div class="forum">
  <h3>Forum</h3>
  <div class="thread-header">
    <div>Title</div>
    <div>Nickname</div>
    <div>Date</div>
    <div>View</div>
    <div>Like</div>
  </div>
  {{#customEach custom}}
  <a href="/read/{{this.THREAD_NUM}}" class="link-thread">
    <div class="thread">
      <div class="upper">
        {{#if this.ADMIN_YN}}
        <div class="thread-notice">Notice</div>
        {{/if}}
        {{#if this.PWD_YN}}
        <div class="thread-secret">Password</div>
        {{/if}}
        <div class="thread-subject">{{this.THREAD_SUBJECT}}</div>
        {{#if this.COMMENTS_COUNT}}
        <div class="thread-comments">+{{this.COMMENTS_COUNT}}</div>
        {{/if}}
      </div>
      <div class="lower">
        <div class="thread-writer">{{this.THREAD_WRITER}}</div>
        <div class="tread-date">{{this.INS_DATE}}</div>
        <div class="thread-hits">{{this.THREAD_HITS}}</div>
        <div class="thread-likes">{{this.THREAD_LIKES}}</div>
      </div>
    </div>
  </a>
  {{/customEach}}
  <div class="pagination">
    <ul>
      {{#paginate pagination type="first"}}
      <li {{#if disabled}}class="disabled" {{/if}}><a href="/?p={{n}}">First</a></li>
      {{/paginate}}
      {{#paginate pagination type="previous"}}
      <li {{#if disabled}}class="disabled" {{/if}}><a href="?p={{n}}">Prev</a></li>
      {{/paginate}}
      {{#paginate pagination type="middle" limit="10"}}
      <li {{#if active}}class="active" {{/if}}><a href="?p={{n}}">{{n}}</a></li>
      {{/paginate}}
      {{#paginate pagination type="next"}}
      <li {{#if disabled}}class="disabled" {{/if}}><a href="?p={{n}}">Next</a></li>
      {{/paginate}}
      {{#paginate pagination type="last"}}
      <li {{#if disabled}}class="disabled" {{/if}}><a href="?p={{n}}">Last</a></li>
      {{/paginate}}
    </ul>
  </div>
  <div class="forum-footer">
    <a href="/write">Write</a>
  </div>
</div>