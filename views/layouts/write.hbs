<div class="dbForm">
  <form action="/write" method="POST">
    <div class="line"><label for="writer">작성자: </label><input type="text" name="writer" placeholder="작성자"></div>
    <div class="line"><label for="subject">제목: </label><input type="text" name="subject" placeholder="제목"></div>
    <textarea style="display: none;" name="content" id="" cols="30" rows="10"></textarea>
    <div id="editor">
      <p><br></p>
    </div>
    <button type="submit">업로드</button>
  </form>
</div>

