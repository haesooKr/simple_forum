<div class="dbForm">
  <form action="/write" method="POST">
    <div class="line"><label for="writer">Nickname: </label><input type="text" name="writer" placeholder="Nickname"></div>
    <div class="line"><label for="subject">Title: </label><input type="text" name="subject" placeholder="Title"></div>
    <div class="line"><label for="password">Password: </label><input type="text" name="password" placeholder="Type when you want it to be private"></div>
    <textarea style="display: none;" name="content" id="" cols="30" rows="10"></textarea>
    <div id="editor">
      <p><br></p>
    </div>
    <button type="submit">Upload</button>
    <a href="/">Back</a>
  </form>
</div>

